<h1>Publishers#index</h1>
<div class="container">
<h2> All Publishers </h2>
<table border="1" class="table">

<tr>
    <th> Publisher Name </th>
    <th> Country </th>
</tr>

<% @publishers.each do |publisher| %>

<tr>
<td> <%= publisher.name %> </td>
<td> <%= publisher.country %> </td>


</tr>
<% end %>
</table>
</div>
